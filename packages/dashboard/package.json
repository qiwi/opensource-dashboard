{
  "name": "@qiwi/dashboard",
  "version": "0.0.0",
  "description": "Dashboard for opensource",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "main": "target/es5/index.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target",
    "typings",
    "flow-typed"
  ],
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.(ts|tsx)\"",
    "lint": "eslint src/**/*.{ts,tsx,js}",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@qiwi/pijma-core": "^1.117.1",
    "@qiwi/pijma-desktop": "^1.118.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "react-scripts": "3.4.4",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@qiwi/dashboard-infra": "*"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/opensource-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/opensource-dashboard/issues"
  },
  "homepage": "https://github.com/qiwi/opensource-dashboard#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
